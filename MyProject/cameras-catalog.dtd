        <!ELEMENT display (#PCDATA)>
        <!ELEMENT video (#PCDATA)>
        <!ELEMENT size (#PCDATA)>
        <!ELEMENT weight (#PCDATA)>
        <!ELEMENT battery (#PCDATA)>
        <!ELEMENT energy_saving (#PCDATA)>
        <!ELEMENT charger (#PCDATA)>
